package com.kh.practice;

import java.io.File;

public class IO00_File {

	public static void main(String[] args) {
		// FIle : 파일 정보를 다루는 클래스
		// = > 파일을 입출력 대상으로 사용할 수 있음!
		
		File file = new File("C:/test.txt");
		System.out.println("> " + file);
		System.out.println("> " + file.toString());
	

	}

}
